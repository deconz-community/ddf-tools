{
  "name": "@deconz-community/directus-extension-ddf-store",
  "type": "module",
  "version": "0.25.1",
  "description": "Extension for Directus to store DDF files in the database, used only by the Deconz Community",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "icon": "extension",
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "add": "directus-extension add"
  },
  "directus:extension": {
    "host": "^10.1.10",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "bundle",
        "source": "src/api/index.ts"
      }
    ]
  },
  "dependencies": {
    "@deconz-community/ddf-bundler": "workspace:^",
    "@directus/errors": "^2.0.4",
    "@noble/hashes": "^2.0.1",
    "@octokit/core": "^7.0.5",
    "@sindresorhus/slugify": "^3.0.0",
    "busboy": "^1.6.0",
    "compare-versions": "^6.1.1",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@directus/api": "^31.0.0",
    "@directus/extensions-sdk": "16.0.2",
    "@directus/types": "^13.2.3",
    "@types/busboy": "^1.5.4",
    "@types/node": "^20.19.23",
    "@types/pako": "^2.0.4",
    "express": "^5.1.0",
    "typescript": "^5.9.3"
  }
}
