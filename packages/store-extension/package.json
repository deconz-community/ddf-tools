{
  "name": "@deconz-community/directus-extension-ddf-store",
  "type": "module",
  "version": "0.26.0",
  "description": "Extension for Directus to store DDF files in the database, used only by the Deconz Community",
  "author": {
    "name": "Zehir",
    "email": "zehir@zorim.fr",
    "url": "https://github.com/Zehir"
  },
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/Zehir"
  },
  "homepage": "https://github.com/deconz-community/ddf-tools/tree/main/packages/store-extension",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deconz-community/ddf-tools.git"
  },
  "bugs": {
    "url": "https://github.com/deconz-community/ddf-tools/issues/"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "icon": "extension",
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "add": "directus-extension add"
  },
  "directus:extension": {
    "host": "^10.1.10",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "bundle",
        "source": "src/api/index.ts"
      }
    ]
  },
  "dependencies": {
    "@deconz-community/ddf-bundler": "workspace:^",
    "@directus/errors": "^2.1.0",
    "@noble/hashes": "^2.0.1",
    "@octokit/core": "^7.0.6",
    "@sindresorhus/slugify": "^3.0.0",
    "busboy": "^1.6.0",
    "compare-versions": "^6.1.1",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@directus/api": "^33.0.0",
    "@directus/extensions-sdk": "17.0.5",
    "@directus/types": "^14.0.0",
    "@types/busboy": "^1.5.4",
    "@types/node": "^20.19.23",
    "@types/pako": "^2.0.4",
    "express": "^5.2.1",
    "typescript": "^5.9.3"
  }
}
