{
	"name": "@deconz-community/directus-extension-ddf-bundle-store-main",
	"version": "0.0.1",
	"type": "module",
	"directus:extension": {
		"host": "^10.1.10",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "auth",
				"source": "src/auth/index.ts"
			},
			{
				"type": "hook",
				"name": "load-env",
				"source": "src/load-env/index.ts"
			}
		]
	},
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.10",
		"@directus/types": "^10.1.6",
		"@types/node": "^20.6.3",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@octokit/core": "^5.0.0"
	}
}
