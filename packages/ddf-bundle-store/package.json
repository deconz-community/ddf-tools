    "scripts": {
        "dev": "node ./scripts/directus_dev.cjs",
        "import:schema": "npx directus schema apply ./schema.yaml",
        "export:schema": "npx directus schema snapshot --yes ./schema.yaml && npx indirectus sdk generate --url http://localhost:8055 --token LOCALHOST_TOKEN --fetch && cp ./.directus/generated/client.ts ./packages/main/src/client.d.ts && cp ./.directus/generated/client.ts ../toolbox/src/interfaces/ddf-bundle-store.d.ts && rm -rf ./.directus",
        "start": "npx directus start"
    },
    "engines": {
        "pnpm": ">=8.6.8",
        "node": ">=16.0.0"
    },
    "packageManager": "pnpm@8.7.5",
    "dependencies": {
        "@deconz-community/directus-extension-ddf-bundle-store-main": "workspace:^",
        "directus": "^10.6.2",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "concurrently": "^8.2.1",
        "nodemon": "^3.0.1",
        "spawn-command": "0.0.2-1"
    }
