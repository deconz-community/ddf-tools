{
    "name": "@deconz-community/directus-extension-store",
    "version": "0.0.1",
    "type": "module",
    "directus:extension": {
        "host": "^10.1.10",
        "type": "bundle",
        "path": {
            "app": "dist/app.js",
            "api": "dist/api.js"
        },
        "entries": [
            {
                "type": "hook",
                "name": "auth",
                "source": "src/auth/index.ts"
            },
            {
                "type": "hook",
                "name": "upload",
                "source": "src/upload/index.ts"
            }
        ]
    },
    "private": true,
    "description": "Please enter a description for your extension",
    "icon": "extension",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-bundle"
    ],
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "add": "directus-extension add"
    },
    "dependencies": {
        "@deconz-community/ddf-bundler": "workspace:^",
        "@directus/api": "^13.1.1",
        "@directus/errors": "^0.0.2",
        "@noble/hashes": "^1.3.2",
        "@octokit/core": "^5.0.1"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "10.1.11",
        "@types/node": "^20.7.1",
        "typescript": "^5.2.2"
    }
}
